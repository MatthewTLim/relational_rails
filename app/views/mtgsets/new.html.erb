<form action="/mtgsets" method="post">
  <label for="set_name">Set Name:</label>
  <input type="text" id="set_name" name="mtgset[set_name]">

  <label for="collection_number">Collection Number:</label>
  <input type="number" id="collection_number" name="mtgset[collection_number]">

  <label for="current_set">Current Set:</label>
  <input type="checkbox" id="current_set" name="mtgset[current_set]">

  <input type="submit" value="Create Set">
</form>